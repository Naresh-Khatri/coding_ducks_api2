{
  "name": "coding_ducks_backend",
  "version": "1.0.0",
  "description": "Backend for Coding Ducks",
  "main": "index.ts",
  "scripts": {
    "dev": "concurrently -r 'yarn dev:yjs-server' 'yarn dev:app'",
    "dev:app": "nodemon index.ts",
    "dev:yjs-server": "nodemon yjs/index.ts",
    "build": "rimraf dist && swc . -d dist && mkdir -p dist/turbodrive/.tmp",
    "start": "yarn build && concurrently -r 'node dist/yjs/index.js' 'node dist/index.js'",
    "test": "vitest --ui"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5",
    "axios": "^0.27.2",
    "compile-code": "^1.1.1",
    "compile-run": "^2.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "firebase-admin": "^11.3.0",
    "helmet": "^7.1.0",
    "imagekit": "^4.1.2",
    "lib0": "^0.2.90",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "node-html-to-image": "^4.0.0",
    "python-shell": "^5.0.0",
    "socket.io": "^4.6.1",
    "vitest": "^0.34.5",
    "y-mongodb": "^0.1.11",
    "y-mongodb-provider": "^0.1.9",
    "y-protocols": "^1.0.6",
    "y-websocket": "^2.0.2",
    "yjs": "^13.6.12"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/cors": "^2.8.12",
    "@types/express-fileupload": "^1.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.2.3",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "prisma": "5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}
